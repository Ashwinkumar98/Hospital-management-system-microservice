spring:
  application:
    name: consultation-service
  cache:
    cache-names:
    - patientCache
    - doctorCache
    - diagnosisCache
    type: ehcache
    ehcache:
      config: classpath:ehcache.xml
    
management:
  endpoints:
    web:
      exposure:
        include:
        - info
        - refresh
        - hystrix.stream